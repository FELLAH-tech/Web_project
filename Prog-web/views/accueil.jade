doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
    meta(name='description' content='')
    meta(name='author' content='')
    link(href='css/bootstrap.css' rel='stylesheet')
    link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css')
    link(rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.9.0/css/all.css')
    link(href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js")
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1' crossorigin='anonymous')

  body
   .bg-image(style="background-image: url('https://mdbcdn.b-cdn.net/img/Photos/Others/images/76.jpg');")
    // Navigation
    nav.navbar.navbar-expand-md.navbar-dark.bg-dark
      .container
            ul.navbar-nav.ml-auto
                <img class="rounded-circle" src= "data:image/jpeg;base64," alt="" width="80" height="80" />
            ul.navbar-nav.mr-auto
                button.navbar-toggler.collapsed(type='button' data-toggle='collapse' data-target='#navbarResponsive' aria-controls='navbarResponsive' aria-expanded='false' aria-label='Toggle navigation')
                  span.navbar-toggler-icon
                #navbarResponsive.navbar-collapse.collapse(style='')
                    li.nav-item.dropdown
                        a.nav-link(href='/my_profil') Profil
                    li.nav-item.dropdown
                        a.nav-link(href='/') Home
                    li.nav-item.dropdown
                       a.nav-link(href='/logout') Sign out

    // Page Content
    br
    .container
      .row
        .col-lg-3
          
          h1.my-4= 'Bonjour ' + data_1.my_user[0].user + ' !'
          .card(style='width: 15rem;')
            .card-header
              i.fas.fa-bell.fa-lg 
                  | Notifications !
            ul.list-group.list-group-flush
              each val in data_1.list
                if( data_1.notif_post_user[val])
                  li.list-group-item= data_1.notif_post_user[val].post_owner + ' has posted !'
              
        .col-lg-9
          .shadow.p-3.mb-5.bg-white.rounded 
            .card-body
              h3.card-title Create a post : 
              form(action='/posts' method='post')
                textarea.input.form-control.ml-1.shadow-none.textarea(name= 'my_post' placeholder='Add a post')
                br
                select.form-select(aria-label='Default select example')
                  option(selected='') Select the category of the link
                  option(value='1') Sport
                  option(value='2') Science
                  option(value='3') Advertisement
                  option(value='4') News
                br
                button.a.btn.btn-primary(type='submit')
                    span.fa.fa-send(style='color:#4f4;')
                    |  Post
          .shadow.p-3.mb-2.bg-white.rounded 
            .card.text-center
              .card-header
                ul.nav.nav-tabs.card-header-tabs
                  li.nav-item
                    a.nav-link.active(href='#') Recently Posted
                  li.nav-item
                    a.nav-link(href='#')
                      i.fas.fa-fire.fa-lg 
                      | Popular Posts
                  li.nav-item
                    a.nav-link(href='#') 
                      i.fas.fa-biking.fa-lg 
                      | Sport
                  li.nav-item
                    a.nav-link(href='#') 
                      i.fas.fa-microscope.fa-lg 
                      | Science
                  li.nav-item
                    a.nav-link(href='#') 
                      i.fas.fa-headphones.fa-lg 
                      | Advertisement
                  li.nav-item
                    a.nav-link(href='#') 
                      i.fas.fa-newspaper.fa-lg 
                      | News

              .card-body
                ul
                  each val in data_1.list
                    hr(size='8' align='right')
                    li
                      .d-flex.justify-content-center.row
                        .col-md-8
                          .d-flex.flex-column.comment-section
                            .bg-white.p-2
                              .d-flex.flex-row.user-info
                                img.rounded-circle(src='data:image/png;base64,' width='40')
                                .d-flex.flex-column.justify-content-start.ml-1
                                  span.h3.d-block.font-weight-bold.name= data_1.the_user[val].post_owner
                                  if(data_1.current_date[val])
                                    span.date.text-black-50= "Shared publicly - " + data_1.current_date[val].post_date
                              .mt-2
                              hr
                              if(data_1.the_post[val])
                                p= data_1.the_post[val].post
                                  
                            hr
                            .bg-white
                              .d-flex.flex-row.fs-12
                                button.btn.btn-outline-success.my-2.my-sm-0(type='button')
                                  span.fas.fa-angle-double-up(style='color:#4f4;') Vote Up
                                button.btn.btn-outline-danger.my-2.my-sm-0(type='button')
                                  span.fas.fa-angle-double-down(style='color:$red-500;') Vote Down     
                              hr 
                              button.btn.btn-outline-primary.my-2.my-sm-0(type='button' data-toggle='collapse' data-target='#collapseExample' aria-expanded='false' aria-controls='collapseExample') 
                                    span.fa.fa-comment(style='color:#adb5bd;')
                                    | Comments 
                                    span.badge.bg-secondary 4
                                #collapseExample.collapse
                                  .shadow-lg.p-3.mb-5.bg-white.rounded 
                                          .card-header
                                            .d-flex.flex-row.user-info
                                              img.rounded-circle(src='data:image/png;base64,' width='40')
                                              .d-flex.flex-column.justify-content-start.ml-2
                                                span.d-block.font-weight-bold.name= data_1.the_user_comm[0].comment_owner
                                                span.date.text-black-50= "Shared publicly - " + data_1.current_date[val].post_date
                                          .card-body
                                          
                                            if(data_1.the_comment[0])
                                              p= data_1.the_comment[0].comment
                                           
                                            hr
                                            .bg-white
                                              .d-flex.flex-row.fs-12
                                                form.vote-up(data-id='{{this._id}}')
                                                    button(type='submit') Vote Up
                                                form.vote-down(data-id='{{this._id}}')
                                                    button(type='submit') Vote Down     
                                              hr 
                                            .d-flex.flex-row.align-items-start
                                              img.rounded-circle(src='data:image/png;base64,' width='40')
                                              form(action='/comment' method='post')
                                                textarea.input.form-control.ml-1.shadow-none.textarea(name= 'my_comment' placeholder='Add a Comment')
                                                button.btn.btn-primary.btn-sm.shadow-none(type='submit') Reply 
                                    br                           
                    hr(size='8' align='right')                            
    // Footer
    footer.py-5.bg-dark
      .container
        p.m-0.text-center.text-muted Copyright © FELLAH Hicham | BIGI Mohamed | 2021
    script(src='https://code.jquery.com/jquery-3.5.1.slim.min.js' integrity='sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj' crossorigin='anonymous')
    script(src='https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js' integrity='sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx' crossorigin='anonymous')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js')
    script(src='https://code.jquery.com/jquery-2.1.3.min.js')
    script(rel='script' src='/vote.js')
